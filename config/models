
Profile json
    name Text
    nickname Text
    primary Bool
    UniqueProfile name
    deriving Show
    deriving Eq

Handle json
    handle Text
    service Service
    profileId ProfileId
    deriving Show
    deriving Eq

ServiceInfo json
    service Service
    isActive Bool default=False
    accessToken Text Maybe
    accessSecret Text Maybe
    UniqueService service
    deriving Show
    deriving Eq

AdiumMessage json
    adiumId Text
    createdAt UTCTime
    senderHandleId HandleId
    recipientHandleId HandleId
    content Text
    UniqueAdiumId adiumId
    deriving Show
    deriving Eq

Journal json
    sender ProfileId
    date UTCTime
    content Text
    deriving Show
    deriving Eq

NoteMessage json
    filePath FilePath
    createdAt UTCTime
    content Text
    raw Text
    UniqueFilePath filePath
    deriving Show
    deriving Eq

TwitterMessage json
    twitterId Int
    createdAt UTCTime
    senderScreenName Text
    senderHandleId HandleId
    recipientScreenName Text
    recipientHandleId HandleId
    content Text
    raw Text
    UniqueTwitterId twitterId
    deriving Show
    deriving Eq
